  "info": {
    "title": "Shrtnr API",
    "description": "The Shrtnr API",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateUrlDto": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "The URL to shorten",
            "example": "https://www.google.com"
          },
          "slug": {
            "type": "string",
            "description": "The slug of the URL",
            "example": "xHy48Ck"
          }
        },
        "required": [
          "url"
        ]
      },
      "Url": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The ID of the URL",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          },
          "url": {
            "type": "string",
            "description": "The URL",
            "example": "https://www.google.com"
          },
          "slug": {
            "type": "string",
            "description": "The slug of the URL",
            "example": "xHy48Ck"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the URL was created",
            "example": "2021-01-01T00:00:00.000Z",
            "readOnly": true
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the URL was updated",
            "example": "2021-01-01T00:00:00.000Z",
            "readOnly": true
          },
          "deletedAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the URL was deleted",
            "example": "2021-01-01T00:00:00.000Z",
            "readOnly": true
          },
          "userId": {
            "type": "string",
            "description": "The ID of the user who created the URL",
            "example": "123e4567-e89b-12d3-a456-426614174000",
            "readOnly": true
          }
        },
        "required": [
          "id",
          "url",
          "slug",
          "createdAt",
          "updatedAt"
        ]
      },
      "UpdateSlugDto": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string",
            "description": "The slug of the URL",
            "example": "new-slug"
          }
        },
        "required": [
          "slug"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The ID of the user",
            "example": "123e4567-e89b-12d3-a456-426614174000",
            "readOnly": true
          },
          "email": {
            "type": "string",
            "description": "The email of the user",
            "example": "john.doe@example.com",
            "readOnly": true
          },
          "name": {
            "type": "string",
            "description": "The name of the user",
            "example": "John Doe",
            "readOnly": true
          },
          "providerId": {
            "type": "string",
            "description": "The provider ID of the user",
            "example": "123e4567-e89b-12d3-a456-426614174000",
            "readOnly": true
          },
          "avatarUrl": {
            "type": "string",
            "description": "The avatar URL of the user",
            "example": "https://example.com/avatar.png",
            "readOnly": true
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the user was created",
            "example": "2021-01-01T00:00:00.000Z",
            "readOnly": true
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the user was updated",
            "example": "2021-01-01T00:00:00.000Z",
            "readOnly": true
          }
        },
        "required": [
          "id",
          "email",
          "name",
          "providerId",
          "avatarUrl",
          "createdAt",
          "updatedAt"
        ]
      },
      "UrlTracking": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The ID of the URL tracking",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          },
          "urlId": {
            "type": "string",
            "description": "The ID of the URL",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          },
          "referrerDomain": {
            "type": "string",
            "description": "The referrer domain",
            "example": "https://www.google.com"
          },
          "browser": {
            "type": "string",
            "description": "The browser",
            "example": "Chrome"
          },
          "operatingSystem": {
            "type": "string",
            "description": "The operating system",
            "example": "Windows"
          },
          "deviceType": {
            "type": "string",
            "description": "The device type",
            "example": "desktop"
          },
          "language": {
            "type": "string",
            "description": "The language",
            "example": "en"
          },
          "ipAddress": {
            "type": "string",
            "description": "The IP address",
            "example": "127.0.0.1"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the URL tracking was created",
            "example": "2021-01-01T00:00:00.000Z"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "description": "The date and time the URL tracking was created",
            "example": "2021-01-01T00:00:00.000Z"
          }
        },
        "required": [
          "id",
          "urlId",
          "referrerDomain",
          "browser",
          "operatingSystem",
          "deviceType",
          "language",
          "ipAddress",
          "createdAt",
          "updatedAt"
        ]
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
