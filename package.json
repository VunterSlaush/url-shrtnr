  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=api",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "build:api": "turbo run build --filter=api",
    "build:api-client": "npx @hey-api/openapi-ts -i ./apps/api/openapi.json -o packages/api-client",
    "test": "turbo run test",
    "test:web": "turbo run test --filter=web",
    "test:api": "turbo run test --filter=api",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:web": "turbo run test:e2e --filter=web",
    "test:e2e:api": "turbo run test:e2e --filter=api",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "@hey-api/openapi-ts": "^0.82.0",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18"
  }
}
